@model IEnumerable<Datingsida.Models.ProfileModel>

@{
    ViewData["Title"] = "Search";
}

<h1>Sök efter profiler</h1>
<form asp-action="SearchResults">
    <div class="form-group">
        <label for="SearchTerm" class="control-label"></label>
        <input name="SearchTerm" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Sök" class="btn btn-primary" />
    </div>
</form>
<div class="card-deck">
    @foreach (var profile in Model)
    {
        <partial name="_DisplayProfilePartial" model="profile" view-data="ViewData" />
    }
</div>